        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.gdutdb.carsales.mapper.OptionMapper">

    <resultMap id="BaseResultMap" type="com.gdutdb.carsales.po.poja.Option">
